{
    "name": "baethon/symfony-console-input",
    "description": "Generates command signature based on an input DTO",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.4",
        "symfony/console": "^7.1"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "pestphp/pest": "^3.2",
        "symfony/var-dumper": "^7.1"
    },
    "authors": [
        {
            "name": "Radoslaw Mejer",
            "email": "radmen@baethon.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Baethon\\Symfony\\Console\\Input\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
